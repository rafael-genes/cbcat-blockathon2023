{
  "name": "blockathon2023",
  "version": "0.0.7",
  "description": "blockathon2023 serverless based backend with NestJS",
  "scripts": {
    "nest:watch": "rimraf dist && nest build --watch",
    "start:dev": "env-cmd -f ./env/.env.development nest start --watch",
    "serverless:watch": "env-cmd -f ./env/.env serverless offline --reloadHandler",
    "serverless:watch:prod": "env-cmd -f ./env/.prod.env serverless offline --reloadHandler",
    "nest": "rimraf dist && nest build",
    "nest:webpack": "nest build --webpack",
    "serverless": "env-cmd -f ./env/.env serverless offline",
    "deploy": "env-cmd -f ./env/.env serverless deploy",
    "deploy:prod": "env-cmd -f ./env/.prod.env serverless deploy --stage prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker": "cd docker && docker compose up",
    "process": "pm2 start 'npm run serverless:watch' && pm2 start 'npm run ngrok:access'",
    "ngrok:access": "../../../ngrok/ngrok http --domain=act-blockathon.ngrok.app --region=eu 3000",
    "ngrok:db": "../../../ngrok/ngrok http --domain=act-blockathon-db.ngrok.app --region=eu 3001"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bloock/sdk": "^2.7.0-beta.14",
    "@ethereumjs/common": "^4.1.0",
    "@ethereumjs/rlp": "^5.0.1",
    "@ethereumjs/tx": "^5.1.0",
    "@ethereumjs/util": "^9.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.1.0",
    "@nestjs/typeorm": "^10.0.1",
    "@openzeppelin/defender-sdk": "^1.7.0",
    "@types/multer": "^1.4.7",
    "@vendia/serverless-express": "^4.10.1",
    "bcrypt": "^5.1.1",
    "bignumber.js": "^9.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "clean-webpack-plugin": "^4.0.0",
    "crypto": "^1.0.1",
    "ethers": "5.7",
    "mysql2": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/aws-lambda": "^8.10.114",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "4.17.17",
    "aws-lambda": "^1.0.7",
    "env-cmd": "^10.1.0",
    "npm-check-updates": "^16.14.12",
    "rimraf": "4.4.1",
    "serverless": "^3.29.0",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^12.0.4",
    "serverless-webpack": "^5.11.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3"
  }
}
